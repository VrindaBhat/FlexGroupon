<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
        xmlns:s="library://ns.adobe.com/flex/spark">
    <fx:Declarations>
        <s:SkinnablePopUpContainer id="viewNavigatorPopUp" height="{height}" width="401" skinClass="skins.MasterNavigatorPopUpSkin" />
    </fx:Declarations>
    
    <s:states>
        <s:State name="portrait" />
        <s:State name="landscape" />
    </s:states>
    
    <!-- Draw a solid black rectangle behind the content group. This will
         only show through as a line between the master and detail navigators. -->
    <s:Rect left="0" right="0" top="0" bottom="0">
        <s:fill>
            <s:SolidColor color="0" />
        </s:fill>
    </s:Rect>
    
    <s:HGroup id="contentGroup" left="0" top="0" right="0" bottom="0" gap="1" />
</s:Skin>
